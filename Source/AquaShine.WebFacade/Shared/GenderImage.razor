@using AquaShine.WebSupport

@code {
   [Parameter] public Gender Gender { get; set; }
}

@switch (Gender)
{
    case Gender.Male:
        <img alt="Male" src="data:image/png;base64,"/>
        break;
    case Gender.Female:
        <img alt="Female" src="data:image/png;base64,"/>
        break;
    case Gender.Other:
        <img alt="Other" src="data:image/png;base64,"/>
        break;
    default:
        throw new ArgumentOutOfRangeException();
}